<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="bootstrap.php"
         backupGlobals="false"
         backupStaticAttributes="false"
         strict="false"
         verbose="true">
    <testsuites>
        <testsuite name="mockster">
            <directory suffix="Test.php">spec</directory>
        </testsuite>
    </testsuites>
    <logging>
        <log type="tap" target="report.tap"/>
    </logging>
</phpunit>
