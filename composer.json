{
    "name": "rtens/mockster",
    "description": "PHP Mocking framework",
    "keywords": ["mocking", "mock", "unit testing", "bdd"],
    "homepage": "http://github.com/rtens/mockster",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolas Martens",
            "email": "Nikolas.M@rtens.org",
            "homepage": "http://rtens.org"
        }
    ],
    "require": {
        "php": ">=5.5",
        "watoki/factory": "1.*",
        "watoki/reflect": "1.*"
    },
    "require-dev": {
        "rtens/scrut": "*"
    },
    "autoload": {
        "psr-4": {
            "rtens\\mockster\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\rtens\\mockster\\": "spec/"
        }
    }
}
